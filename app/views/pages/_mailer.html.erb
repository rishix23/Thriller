<div class="row">
 <div class="col-md-12 col-md-offset-12">
   <div class="well">
      <%= simple_form_for @contact do |f| %>
      <%= f.input :name %>
      <%= f.input :email %>
      <%= f.input :message %>
      <%= f.submit "Get in touch!", class: "btn btn-info" %>
     <% end %>
   </div>
 </div>
</div>